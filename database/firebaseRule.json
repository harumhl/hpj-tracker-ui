{
  "rules": {
    ".read": "true",
    ".write": "auth.uid === ''",
    "categories": {
      "category": {".validate": "newData.isString()"},
      "$other":   {".validate": false}
    },
    "goals": {
      "archived":  {".validate": "newData.isBoolean()"},
      "category":  {".validate": "newData.isString()"},
      "goalCount": {".validate": "newData.isNumber()"},
      "name":      {".validate": "newData.isString()"},
      "unit":      {".validate": "newData.isString()"},
      "$other":    {".validate": false}
    },
    "entries": {
      "category":  {".validate": "newData.isString()"},
      "count":     {".validate": "newData.isNumber()"},
      "doneDate":  {".validate": "newData.isString()"},
      "goalCount": {".validate": "newData.isNumber()"},
      "name":      {".validate": "newData.isString()"},
      "updatedTs": {".validate": "newData.isString()"},
      "$other":    {".validate": false}
    }
  }
}
